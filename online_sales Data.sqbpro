    STRFTIME('%Y', &quot;Date&quot;) AS year,
    STRFTIME('%m', &quot;Date&quot;) AS month,
	STRFTIME('%Y-%m', &quot;Date&quot;) AS year_month,#additional representation 
    SUM(&quot;Total Revenue&quot;) AS total_revenue,
    COUNT(DISTINCT &quot;Transaction ID&quot;) AS order_volume
FROM
