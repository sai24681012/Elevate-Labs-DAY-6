    SUM(&quot;Total Revenue&quot;) AS regional_revenue
FROM
    orders
GROUP BY
    &quot;Region&quot;, year_month
ORDER BY
    &quot;Region&quot;, year_month;</sql><sql name="YEAR BASED*">SELECT
    &quot;Payment Method&quot;,
